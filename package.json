{
    "name": "juliadocs",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "cat static/data/dev/documents.json | node buildindex.cjs > static/data/dev/searchindex.json && http-server static --cors & svelte-kit build",
        "preview": "svelte-kit preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "buildindex": "node buildindex.js dev"
    },
    "devDependencies": {
        "@sveltejs/kit": "^1.0.0-next.260",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "autoprefixer": "^10.4.2",
        "carbon-icons-svelte": "^10.44.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^3.4.0",
        "http-server": "^14.1.0",
        "postcss": "^8.4.6",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "svelte": "^3.46.4",
        "svelte-check": "^2.4.3",
        "svelte-media": "^0.1.5",
        "svelte-preprocess": "^4.10.2",
        "tailwindcss": "^3.0.19",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5"
    },
    "type": "module",
    "dependencies": {
        "@sveltejs/adapter-static": "^1.0.0-next.26",
        "@types/lunr": "^2.3.4",
        "highlight.js": "^11.4.0",
        "lunr": "^2.3.9",
        "sass": "^1.49.7",
        "svelte-fsm": "^1.1.2"
    }
}
